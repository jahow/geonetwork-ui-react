{
  "name": "gn-ui-react",
  "version": "0.0.1",
  "description": "POC for GeoNetwork UI components based on React",
  "scripts": {
    "dev:app": "webpack-dev-server --config app/webpack.config.js --mode development",
    "build:app": "webpack -p --config app/webpack.config.js",
    "dev:elements": "webpack-dev-server --config elements/webpack.config.js --mode development",
    "build:elements": "webpack -p --config elements/webpack.config.js",
    "format": "prettier \"**/*.{tsx,html}\" --write"
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "ol": "6.3.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "css-loader": "^3.5.3",
    "prettier": "^2.0.5",
    "source-map-loader": "^1.0.0",
    "style-loader": "^1.2.1",
    "to-string-loader": "^1.1.6",
    "ts-loader": "^7.0.5",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.9.3",
    "webpack": "^4.11.0",
    "webpack-cli": "^3.0.2",
    "webpack-dev-server": "^3.1.4"
  }
}
